<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush Color="Red" x:Key="color1"/>
    
    <SolidColorBrush Color="Yellow" x:Key="color2"/>
    
    <LinearGradientBrush  x:Key="sunriseBrush"  EndPoint="0.5,1"  StartPoint="0.5,0">
        <GradientStop  Color="#FF12243E"/>
        <GradientStop  Color="#FF95229B"  Offset="0.936"/>
        <GradientStop  Color="#FF232D69"  Offset="0.520"/>
        <GradientStop  Color="#FFEE55B6"  Offset="0.327"/>
        <GradientStop  Color="#FFF19453"  Offset="0.997"/>
    </LinearGradientBrush>

    <SolidColorBrush Color="White" x:Key="color3"/>

</ResourceDictionary>